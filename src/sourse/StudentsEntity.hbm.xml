<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sourse.StudentsEntity" table="students" schema="myzcy">
        <id name="sid">
            <column name="sid" sql-type="int(11)"/>
        </id>
        <property name="sname">
            <column name="sname" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="char(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>